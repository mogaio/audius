{
  "name": "audius",
  "version": "2.0.12",
  "author": "Falko Krause",
  "description": "Play media files posted in conversations",
  "main": "index.js",
  "scripts": {
    "start": "node ./config/webpack.devserver.js",
    "test": "eslint ./src/**/*.js",
    "build:prod:app": "./node_modules/.bin/webpack --config='./config/webpack.production.app.js' --progress --display-error-details && ./src/scripts/single-file-release.js",
    "build": "./node_modules/.bin/webpack --config='./config/webpack.base.js' --progress --display-error-details",
    "build:extension": "./node_modules/.bin/webpack --config='./config/webpack.dev.extension.js' --progress --display-error-details --watch",
    "docu": "livingstyleguide compile ./src/sass/styleguide.lsg ./documentation/styleguide.html && nodejs node_modules/.bin/generate-md --layout github --input README.md --output ./documentation/",
    "extension:rls": "npm run version; npm run build:prod:extension; cd dist-extension/; zip -r ../audius.extension.zip . *",
    "version": "src/scripts/bump-version.js",
    "build:prod:extension": "./node_modules/.bin/webpack --config='./config/webpack.production.extension.js' --progress --display-error-details",
    "icons": "./config/convert-icomoon.js ./src/icons/media-player.zip ./src/sass/_icon.map.sass ./src/fonts/",
    "serve-static": "http-server -p 8080 ./dist-website"
  },
  "license": "MIT",
  "private": true,
  "dependencies": {
    "matrix-js-sdk": "^0.10.6",
    "sortablejs": "^1.7.0",
    "vue": "^2.5.16",
    "vuedraggable": "^2.16.0",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "adm-zip": "^0.4.11",
    "autoprefixer": "^7.2.3",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "circular-dependency-plugin": "^4.3.0",
    "compression-webpack-plugin": "^1.1.11",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^0.28.7",
    "eslint": "^4.14.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-html": "^4.0.5",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "file-loader": "^1.1.11",
    "http-server": "^0.11.1",
    "markdown-styles": "^3.1.10",
    "node-sass": "^4.9.2",
    "postcss-loader": "^2.1.6",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.21.0",
    "sw-precache-webpack-plugin": "^0.11.5",
    "url-loader": "^0.6.2",
    "vue-hot-reload-api": "^2.3.0",
    "vue-loader": "^13.6.1",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7"
  }
}
